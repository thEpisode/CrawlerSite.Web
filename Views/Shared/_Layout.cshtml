@RenderSection("scripts", required: false)

	<script>
		 $('.dropdown-menu>.feedback-container').click(function(e) {
			e.stopPropagation();
		});

		$('.face').click(function(e){
			
			e.preventDefault();

			$('.face').removeClass('face-selected');
			$(this).addClass('face-selected');
		});

		$('.send-feedback').click(function(e){
			e.preventDefault();

			var data = {
				UserId: $Flinger.ReadPersistentData('userId'),
				Path: $Flinger.GetEntryPath(),
				Like: $('.face').data('like'),
				Description: $('#comment').val(),
                Logs: $Flinger.Logs,
				ReportBug: $('#bug').val(),
                Version: $Flinger.Version
			}

			$.ajax({
				url: "@Url.Action("Send", "Feedback")",
				beforeSend: function(request) {
					request.setRequestHeader("x-access-token", $Flinger.ReadPersistentData('auth_token'));
				},
				type: "GET",
				data: data,
				dataType: "json"
			});
		});

		
	</script>
