				</div>

				<ul class="nav navigation-handler">
					<li class="active" data-path="home">
						<a href="@Url.Action("Index", "Dashboard")">
							<i class="ti-home"></i>
							<p>Home</p>
						</a>
					</li>
					<li data-path="add-site">
						<a href="@Url.Action("Add", "Site")">
							<i class="ti-plus"></i>
							<p>Add Site</p>
						</a>
					</li>
					<li data-path="site">
						<a href="@Url.Action("Index", "Site")">
							<i class="ti-world"></i>
							<p>Sites</p>
						</a>
					</li>
					<li data-path="user">
						<a href="@Url.Action("Index", "User")">
							<i class="ti-user"></i>
							<p>Users</p>
						</a>
					</li>
					<li data-path="ip">
						<a href="@Url.Action("Index", "Ip")">
							<i class="ti-filter"></i>
							<p>Connection Filtering</p>
						</a>
					</li>
					<li data-path="billing">
						<a href="@Url.Action("Index", "Billing")">
							<i class="ti-credit-card"></i>
							<p>Billing</p>
						</a>
					</li>

					<li class="active-pro">
						<a href="javascript: $Flinger.LogOut()">
							<i class="ti-close"></i>
							<p>Exit</p>
						</a>
					</li>
				</ul>
			</div>
		</div>

		<div class="main-panel">
			<nav class="navbar navbar-default">
				<div class="container-fluid">
					<div class="navbar-header">
						<button type="button" class="navbar-toggle">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar bar1"></span>
                            <span class="icon-bar bar2"></span>
                            <span class="icon-bar bar3"></span>
                        </button>
						<!--<a class="navbar-brand" href="#">Dashboard</a>-->
					</div>
					<div class="collapse navbar-collapse">
						<ul class="nav navbar-nav navbar-right">
							<li class="dropdown" ng-controller="NotificationController" ng-init="InitializeIndexView()">
								<a href="#" class="dropdown-toggle" data-toggle="dropdown">
									<i class="ti-bell"></i>
									<p>Notifications</p>
									<b class="caret"></b>
								</a>
								<ul class="dropdown-menu" ng-show="NoNotifications">
									<li><a href="#">You have no notifications</a></li>
								</ul>
								<ul class="dropdown-menu" ng-hide="NoNotifications">
									<li ng-repeat="Notification in Notifications" ng-show="$index<10"><a href="@Url.Action("Detail", "Notification")?Id={{Notification._id}}">{{Notification.ShortMessage}}</a></li>
									<div class="separator"></div>
									<li><a href="@Url.Action("Index", "Notification")">View All</a></li>
								</ul>
							</li>
							<li class="dropdown">
								<a href="#" class="dropdown-toggle feedback-link" data-toggle="dropdown">
									<i class="ti-face-smile"></i>
									<p>Feedback</p>
									<b class="caret"></b>
								</a>
								<section class="dropdown-menu">
									<article class="feedback-container">
										<h3 class="text-center">Tell us your feedback</h3>
										<div>
											<div class="form-group">
												<label>How was your experience?</label>
												<div class="experience-face text-center">
													<a href="" class="face" title="great experience" data-like="true"><i class="ti-face-smile"></i></a>
													<a href="" class="face" title="bad experience" data-like="false"><i class="ti-face-sad"></i></a>
												</div>
											</div>
											
											<div class="form-group">
												<label for="bug">You have experienced an error?</label>
												<select class="form-control" id="bug">
													<option value="0" disabled selected>Select an option</option>
													<option value="true">Yes</option>
													<option value="false">No</option>
												</select>
											</div>
											<div class="form-group">
												<label>Tell us why?</label>
												<textarea class="form-control" rows="3" id="comment"></textarea>
											</div>
											
											<a href="#" class="btn btn-success pull-right send-feedback">Send</a>
										</div>
									</article>
								</section>
							</li>
							<li>
								<a href="@Url.Action("Index", "Account")">
									<img class="avatar-header avatar border-gray-light" src="@Url.Content("~/dashboard-assets/img/faces/face-0.jpg")" alt="User Profile"
									/>
									<p>Account</p>
								</a>
							</li>
						</ul>

					</div>
				</div>
